{
    "name": "atk4/monorepo",
    "description": "Monorepo of major Atk4 repos",
    "type": "library",
    "homepage": "https://www.mvorisek.com/",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Voříšek",
            "email": "mvorisek@mvorisek.cz"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "atk4/core": "dev-develop#882cf1c1bc9a8f09f4550185ce30a22b79310750",
        "atk4/dsql": "dev-develop#c3c8c58e898af642a4e9d024eb2e80572b63ce3e",
        "atk4/data": "dev-develop#e5dba07eb7ad1ef974ba0cb40c42c010f2349259",
        "atk4/schema": "dev-develop#8b81b6b0b3d640503bf752e6ed05bdf1d0e08dcd",
        "atk4/ui": "dev-develop#e8bb66411b49f0368941489538270d06aea0e8f8"
    },
    "require-dev": {
        "phpunit/phpunit": "*, <6, <6, <6, <6",
        "symfony/yaml": "~2.1|~3.0|~4.0",
        "satooshi/php-coveralls": "^1.0",
        "symfony/contracts": "^1.1",
        "atk4/data": "dev-develop, dev-develop",
        "fzaninotto/faker": "*, *, *, *, *",
        "phpunit/dbunit": ">=1.2, >=1.2, >=1.2",
        "atk4/schema": "dev-develop",
        "atk4/ui": "dev-develop",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.3",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload-dev": {
        "psr-4": {
            "atk4\\core\\tests\\": [
                "vendor/atk4/core/tests/"
            ],
            "atk4\\dsql\\tests\\": [
                "vendor/atk4/dsql/tests/"
            ],
            "atk4\\data\\tests\\": [
                "vendor/atk4/data/tests/"
            ],
            "atk4\\data\\tests\\smbo\\": [
                "vendor/atk4/data/tests/smbo",
                "vendor/atk4/data/tests/smbo/lib"
            ],
            "atk4\\schema\\tests\\": [
                "vendor/atk4/schema/tests/"
            ],
            "atk4\\ui\\tests\\": [
                "vendor/atk4/ui/tests/"
            ]
        }
    }
}
