{
    "name": "atk4/monorepo",
    "description": "Monorepo of major Atk4 repos",
    "type": "library",
    "homepage": "https://www.mvorisek.com/",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Voříšek",
            "email": "mvorisek@mvorisek.cz"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "atk4/core": "dev-develop#9f58502ba0513208171c2eddc1b804cc9d3656f6",
        "atk4/dsql": "dev-develop#9a081b6682a895f4aa6602f9abb63455e4585cd3",
        "atk4/data": "dev-develop#f4a32944631b4a975a9adb1443a3d76848824faf",
        "atk4/schema": "dev-develop#905d2d2058b53d89b123218bd599a087cd3a0386",
        "atk4/ui": "dev-develop#6f2fba1e7327f8ea39f5d30e649ebc4321d5d158"
    },
    "require-dev": {
        "phpunit/phpunit": "*, <6, <6, *, <6",
        "symfony/yaml": ">=3.4",
        "satooshi/php-coveralls": "^1.0",
        "symfony/contracts": "^1.1",
        "atk4/data": "dev-develop",
        "phpunit/dbunit": ">=1.2, >=1.2",
        "atk4/schema": "dev-develop",
        "atk4/ui": "dev-develop",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.3",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload-dev": {
        "psr-4": {
            "atk4\\core\\tests\\": [
                "vendor/atk4/core/tests/"
            ],
            "atk4\\dsql\\tests\\": [
                "vendor/atk4/dsql/tests/"
            ],
            "atk4\\data\\tests\\": [
                "vendor/atk4/data/tests/"
            ],
            "atk4\\data\\tests\\smbo\\": [
                "vendor/atk4/data/tests/smbo",
                "vendor/atk4/data/tests/smbo/lib"
            ],
            "atk4\\schema\\tests\\": [
                "vendor/atk4/schema/tests/"
            ],
            "atk4\\ui\\tests\\": [
                "vendor/atk4/ui/tests/"
            ]
        }
    }
}
