{
    "name": "atk4/monorepo",
    "description": "Monorepo of major Atk4 repos",
    "type": "library",
    "homepage": "https://www.mvorisek.com/",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Voříšek",
            "email": "mvorisek@mvorisek.cz"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "atk4/core": "dev-develop#54129cc2d772c0609df9ef5dc0b973917dfe8b63",
        "atk4/dsql": "dev-develop#9a081b6682a895f4aa6602f9abb63455e4585cd3",
        "atk4/data": "dev-develop#1ac356a645bcd21eee592c6d088c47eab648e700",
        "atk4/schema": "dev-develop#905d2d2058b53d89b123218bd599a087cd3a0386",
        "atk4/ui": "dev-develop#a220291a1712478cfb90b8dbb01056bdd31c2fbb"
    },
    "require-dev": {
        "phpunit/phpunit": "*, <6, <6, *, *",
        "symfony/yaml": ">=3.4",
        "symfony/contracts": ">=1.1",
        "atk4/data": "dev-develop",
        "phpunit/dbunit": ">=1.2, >=1.2",
        "atk4/schema": "dev-develop",
        "atk4/ui": "dev-develop",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.3 || ^5.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload-dev": {
        "psr-4": {
            "atk4\\core\\tests\\": [
                "vendor/atk4/core/tests/"
            ],
            "atk4\\dsql\\tests\\": [
                "vendor/atk4/dsql/tests/"
            ],
            "atk4\\data\\tests\\": [
                "vendor/atk4/data/tests/"
            ],
            "atk4\\schema\\tests\\": [
                "vendor/atk4/schema/tests/"
            ],
            "atk4\\ui\\tests\\": [
                "vendor/atk4/ui/tests/"
            ]
        }
    }
}
