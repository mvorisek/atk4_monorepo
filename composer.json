{
    "name": "atk4/monorepo",
    "description": "Monorepo of major Atk4 repos",
    "type": "library",
    "homepage": "https://www.mvorisek.com/",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Voříšek",
            "email": "mvorisek@mvorisek.cz"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "atk4/core": "dev-develop#2652d770c37fb29e8663ec2fe0fd60647e8b145e",
        "atk4/dsql": "dev-develop#e59306c65edf6c07e1ef7538d2cee275bbc83329",
        "atk4/data": "dev-develop#f7ebcacc4642a85977dbb0cfdabfffcfb5c0be7d",
        "atk4/schema": "dev-develop#d2f0ae596ce9ab147cd58ca3ed113d0de9b8d4af",
        "atk4/ui": "dev-develop#fe8948a0240585bf21ba3e4bf38a2a439918ce9d"
    },
    "require-dev": {
        "atk4/data": "dev-develop",
        "friendsofphp/php-cs-fixer": "^2.16, ^2.16, ^2.16, ^2.16, ^2.16",
        "phpunit/phpunit": "*, *, *, *, *",
        "symfony/contracts": ">=1.1",
        "fzaninotto/faker": "*, *, *, *, *",
        "atk4/schema": "dev-develop",
        "atk4/ui": "dev-develop",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.3 || ^5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "atk4\\core\\tests\\": [
                "vendor/atk4/core/tests/"
            ],
            "atk4\\dsql\\tests\\": [
                "vendor/atk4/dsql/tests/"
            ],
            "atk4\\data\\tests\\": [
                "vendor/atk4/data/tests/"
            ],
            "atk4\\schema\\tests\\": [
                "vendor/atk4/schema/tests/"
            ],
            "atk4\\ui\\tests\\": [
                "vendor/atk4/ui/tests/"
            ]
        }
    }
}
