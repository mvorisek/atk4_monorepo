{
    "name": "atk4/monorepo",
    "description": "Monorepo of major Atk4 repos",
    "type": "library",
    "homepage": "https://www.mvorisek.com/",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Voříšek",
            "email": "mvorisek@mvorisek.cz"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "atk4/core": "dev-develop#60dcca0273de92f773c02303e6ce570e95a80588",
        "atk4/dsql": "dev-develop#8f2071b30807467806253fc280e6ae19d1b475dc",
        "atk4/data": "dev-develop#4f1afed0eb514d32f8f688ef61e42de9990c231e",
        "atk4/schema": "dev-develop#cbc26c35b81650c997c0139d4a4b0d701773b152",
        "atk4/ui": "dev-develop#26def2029c9c908482767f3928f343f3e2fae42e"
    },
    "require-dev": {
        "atk4/data": "dev-develop",
        "friendsofphp/php-cs-fixer": "^2.16, ^2.16, ^2.16, ^2.16, ^2.16",
        "phpunit/phpunit": "*, *, *, *, *",
        "symfony/contracts": ">=1.1",
        "fzaninotto/faker": "*, *, *, *, *",
        "atk4/schema": "dev-develop",
        "atk4/ui": "dev-develop",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.3 || ^5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "atk4\\core\\tests\\": [
                "vendor/atk4/core/tests/"
            ],
            "atk4\\dsql\\tests\\": [
                "vendor/atk4/dsql/tests/"
            ],
            "atk4\\data\\tests\\": [
                "vendor/atk4/data/tests/"
            ],
            "atk4\\schema\\tests\\": [
                "vendor/atk4/schema/tests/"
            ],
            "atk4\\ui\\tests\\": [
                "vendor/atk4/ui/tests/"
            ]
        }
    }
}
