{
    "name": "atk4/monorepo",
    "description": "Monorepo of major Atk4 repos",
    "type": "library",
    "homepage": "https://www.mvorisek.com/",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Voříšek",
            "email": "mvorisek@mvorisek.cz"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "atk4/core": "dev-develop#c40a81afbbfb737594b3f9868d194d55e2bfd3cc",
        "atk4/dsql": "dev-develop#3f3d82a0d054bc30f825eaea025e2580e3a16b39",
        "atk4/data": "dev-develop#7c8a99b1e3309fe50a08fa97b25f099a16c4e7c1",
        "atk4/schema": "dev-develop#8b81b6b0b3d640503bf752e6ed05bdf1d0e08dcd",
        "atk4/ui": "dev-develop#1dd26424167103a78456fc1107a4c8a5f96d2f9a"
    },
    "require-dev": {
        "phpunit/phpunit": "*, <6, <6, <6, <6",
        "symfony/yaml": "~2.1|~3.0|~4.0",
        "satooshi/php-coveralls": "^1.0",
        "symfony/contracts": "^1.1",
        "atk4/data": "dev-develop, dev-develop",
        "fzaninotto/faker": "*, *, *, *, *",
        "phpunit/dbunit": ">=1.2, >=1.2, >=1.2",
        "atk4/schema": "dev-develop",
        "atk4/ui": "dev-develop",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.3"
    },
    "autoload-dev": {
        "psr-4": {
            "atk4\\core\\tests\\": [
                "vendor/atk4/core/tests/"
            ],
            "atk4\\dsql\\tests\\": [
                "vendor/atk4/dsql/tests/"
            ],
            "atk4\\data\\tests\\": [
                "vendor/atk4/data/tests/"
            ],
            "atk4\\data\\tests\\smbo\\": [
                "vendor/atk4/data/tests/smbo",
                "vendor/atk4/data/tests/smbo/lib"
            ],
            "atk4\\schema\\tests\\": [
                "vendor/atk4/schema/tests/"
            ],
            "atk4\\ui\\tests\\": [
                "vendor/atk4/ui/tests/"
            ]
        }
    }
}
