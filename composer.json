{
    "name": "atk4/monorepo",
    "description": "Monorepo of major Atk4 repos",
    "type": "library",
    "homepage": "https://www.mvorisek.com/",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Voříšek",
            "email": "mvorisek@mvorisek.cz"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "atk4/core": "dev-develop#2d72fedda6604b0f28f9c128acc506f0a5993f23",
        "atk4/dsql": "dev-develop#3dbdff516343d60cfd4550fd5adfe4dbc9ba77a6",
        "atk4/data": "dev-develop#42475e20f5f2ee66376ea062a7650d8c31412259",
        "atk4/schema": "dev-develop#3eaf24afc69fc59fd8060988f8a582135095cf90",
        "atk4/ui": "dev-develop#0f12bacb2b464fa77cd8d9f2e982a84e7f86e005"
    },
    "require-dev": {
        "phpunit/phpunit": "*, <6, <6, <6, <6",
        "symfony/yaml": "~2.1|~3.0|~4.0",
        "satooshi/php-coveralls": "^1.0",
        "symfony/contracts": "^1.1",
        "atk4/data": "dev-develop, dev-develop",
        "fzaninotto/faker": "*, *, *, *, *",
        "phpunit/dbunit": ">=1.2, >=1.2, >=1.2",
        "atk4/schema": "dev-develop",
        "atk4/ui": "dev-develop",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.3"
    },
    "autoload-dev": {
        "psr-4": {
            "atk4\\core\\tests\\": [
                "vendor/atk4/core/tests/"
            ],
            "atk4\\dsql\\tests\\": [
                "vendor/atk4/dsql/tests/"
            ],
            "atk4\\data\\tests\\": [
                "vendor/atk4/data/tests/"
            ],
            "atk4\\data\\tests\\smbo\\": [
                "vendor/atk4/data/tests/smbo",
                "vendor/atk4/data/tests/smbo/lib"
            ],
            "atk4\\schema\\tests\\": [
                "vendor/atk4/schema/tests/"
            ],
            "atk4\\ui\\tests\\": [
                "vendor/atk4/ui/tests/"
            ]
        }
    }
}
