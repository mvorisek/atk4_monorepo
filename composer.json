{
    "name": "atk4/monorepo",
    "description": "Monorepo of major Atk4 repos",
    "type": "library",
    "homepage": "https://www.mvorisek.com/",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Voříšek",
            "email": "mvorisek@mvorisek.cz"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "atk4/core": "dev-develop#2872fdbb38f99df2c01f007f87ab11154e29826f",
        "atk4/dsql": "dev-develop#c78be92a3dcc24a2e5abb63ed08413a462cacc9c",
        "atk4/data": "dev-develop#800b469ac097d2fe1b5e6cfd40ad04b02ff19c58",
        "atk4/schema": "dev-develop#ff01475cedb8ce8fdd8609473f830f8a6e94cf2c",
        "atk4/ui": "dev-develop#31767f82c24db18abf9537c66db9f8296c13c31d"
    },
    "require-dev": {
        "phpunit/phpunit": "*, <6, <6, <6, <6",
        "symfony/yaml": "~2.1|~3.0|~4.0",
        "satooshi/php-coveralls": "^1.0",
        "symfony/contracts": "^1.1",
        "atk4/data": "dev-develop, dev-develop",
        "fzaninotto/faker": "*, *, *, *, *",
        "phpunit/dbunit": ">=1.2, >=1.2, >=1.2",
        "atk4/schema": "dev-develop",
        "atk4/ui": "dev-develop",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.3"
    },
    "autoload-dev": {
        "psr-4": {
            "atk4\\core\\tests\\": [
                "vendor/atk4/core/tests/"
            ],
            "atk4\\dsql\\tests\\": [
                "vendor/atk4/dsql/tests/"
            ],
            "atk4\\data\\tests\\": [
                "vendor/atk4/data/tests/"
            ],
            "atk4\\data\\tests\\smbo\\": [
                "vendor/atk4/data/tests/smbo",
                "vendor/atk4/data/tests/smbo/lib"
            ],
            "atk4\\schema\\tests\\": [
                "vendor/atk4/schema/tests/"
            ],
            "atk4\\ui\\tests\\": [
                "vendor/atk4/ui/tests/"
            ]
        }
    }
}
