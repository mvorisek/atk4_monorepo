{
    "name": "atk4/monorepo",
    "description": "Monorepo of major Atk4 repos",
    "type": "library",
    "homepage": "https://www.mvorisek.com/",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Voříšek",
            "email": "mvorisek@mvorisek.cz"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "atk4/core": "dev-develop#a3a6d8ad21c521f400b4bc414f4dde646fb17618",
        "atk4/dsql": "dev-develop#36e42126b4add4d39761622dfe7c39007eb674ab",
        "atk4/data": "dev-develop#90ab68ac063b8fc2c72dcd66115f1bd3f70a3a92",
        "atk4/schema": "dev-develop#0a8e62c1f067a570f029eee4d48ee7284334cc22",
        "atk4/ui": "dev-develop#ceddc734e031a6512815a6dc84ea020545096d49"
    },
    "require-dev": {
        "atk4/data": "dev-develop",
        "friendsofphp/php-cs-fixer": "^2.16, ^2.16, ^2.16, ^2.16, ^2.16",
        "phpunit/phpunit": "*, *, <6, *, *",
        "symfony/contracts": ">=1.1",
        "symfony/yaml": ">=3.4",
        "fzaninotto/faker": "*, *, *, *, *",
        "atk4/schema": "dev-develop",
        "phpunit/dbunit": ">=1.2",
        "atk4/ui": "dev-develop",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.3 || ^5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "atk4\\core\\tests\\": [
                "vendor/atk4/core/tests/"
            ],
            "atk4\\dsql\\tests\\": [
                "vendor/atk4/dsql/tests/"
            ],
            "atk4\\data\\tests\\": [
                "vendor/atk4/data/tests/"
            ],
            "atk4\\schema\\tests\\": [
                "vendor/atk4/schema/tests/"
            ],
            "atk4\\ui\\tests\\": [
                "vendor/atk4/ui/tests/"
            ]
        }
    }
}
