{
    "name": "atk4/monorepo",
    "description": "Monorepo of major Atk4 repos",
    "type": "library",
    "homepage": "https://www.mvorisek.com/",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Voříšek",
            "email": "mvorisek@mvorisek.cz"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "atk4/core": "dev-develop#47ec4d21a9354aab176609df5cfe88c61d787ac4",
        "atk4/dsql": "dev-develop#7f6ff3d9c8479f42674488b8bb4b31c86d022d22",
        "atk4/data": "dev-develop#c7b6de34cb0280f0d3374ae6587438ae1d693f96",
        "atk4/schema": "dev-develop#008909c8e5802cda4b9a6d736c601fba98e26f67",
        "atk4/ui": "dev-develop#3fd37f0bee0d43f01e9eccb41e96b9dc3466d5ca"
    },
    "require-dev": {
        "atk4/data": "dev-develop",
        "friendsofphp/php-cs-fixer": "^2.16, ^2.16, ^2.16, ^2.16",
        "phpunit/phpunit": "*, <6, <6, *, *",
        "symfony/contracts": ">=1.1",
        "symfony/yaml": ">=3.4",
        "fzaninotto/faker": "*, *, *, *, *",
        "phpunit/dbunit": ">=1.2, >=1.2",
        "atk4/schema": "dev-develop",
        "atk4/ui": "dev-develop",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.3 || ^5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "atk4\\core\\tests\\": [
                "vendor/atk4/core/tests/"
            ],
            "atk4\\dsql\\tests\\": [
                "vendor/atk4/dsql/tests/"
            ],
            "atk4\\data\\tests\\": [
                "vendor/atk4/data/tests/"
            ],
            "atk4\\schema\\tests\\": [
                "vendor/atk4/schema/tests/"
            ],
            "atk4\\ui\\tests\\": [
                "vendor/atk4/ui/tests/"
            ]
        }
    }
}
