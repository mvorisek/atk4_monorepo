{
    "name": "atk4/monorepo",
    "description": "Monorepo of major Atk4 repos",
    "type": "library",
    "homepage": "https://www.mvorisek.com/",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Voříšek",
            "email": "mvorisek@mvorisek.cz"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "atk4/core": "dev-develop#1a158373a82fb467b826fab923c80c38cf87a5fa",
        "atk4/dsql": "dev-develop#543519b43b2d8d08ca055aac7296342a29239980",
        "atk4/data": "dev-develop#8748a634b3ab4f7b02b5a009eaf2c28f917f3cb9",
        "atk4/schema": "dev-develop#bc43225bd3039ba8fbd23b6bfb9790e427bc8f62",
        "atk4/ui": "dev-develop#1da7b5808c735d01f405cc09f9815b3b025c03aa"
    },
    "require-dev": {
        "atk4/data": "dev-develop",
        "friendsofphp/php-cs-fixer": "^2.16, ^2.16, ^2.16, ^2.16, ^2.16",
        "phpunit/phpunit": "*, *, *, *, *",
        "symfony/contracts": ">=1.1",
        "fzaninotto/faker": "*, *, *, *, *",
        "atk4/schema": "dev-develop",
        "atk4/ui": "dev-develop",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.3 || ^5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "atk4\\core\\tests\\": [
                "vendor/atk4/core/tests/"
            ],
            "atk4\\dsql\\tests\\": [
                "vendor/atk4/dsql/tests/"
            ],
            "atk4\\data\\tests\\": [
                "vendor/atk4/data/tests/"
            ],
            "atk4\\schema\\tests\\": [
                "vendor/atk4/schema/tests/"
            ],
            "atk4\\ui\\tests\\": [
                "vendor/atk4/ui/tests/"
            ]
        }
    }
}
