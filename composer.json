{
    "name": "atk4/monorepo",
    "description": "Monorepo of major Atk4 repos",
    "type": "library",
    "homepage": "https://www.mvorisek.com/",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Voříšek",
            "email": "mvorisek@mvorisek.cz"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "atk4/core": "dev-develop#313e372f30eb249ad1375f13c84acef232340a98",
        "atk4/dsql": "dev-develop#1800447f9f15636f69b16655c3c37bd90f4652fd",
        "atk4/data": "dev-develop#c7b6de34cb0280f0d3374ae6587438ae1d693f96",
        "atk4/schema": "dev-develop#5a2c4bad2b4f42b227a2817ef071905a8141a245",
        "atk4/ui": "dev-develop#aa60b9f49c2049e802ef67a6d6fbf96976efe7f1"
    },
    "require-dev": {
        "atk4/data": "dev-develop",
        "friendsofphp/php-cs-fixer": "^2.16, ^2.16",
        "phpunit/phpunit": "*, <6, <6, *, *",
        "symfony/contracts": ">=1.1",
        "symfony/yaml": ">=3.4",
        "fzaninotto/faker": "*, *, *, *, *",
        "phpunit/dbunit": ">=1.2, >=1.2",
        "atk4/schema": "dev-develop",
        "atk4/ui": "dev-develop",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.3 || ^5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "atk4\\core\\tests\\": [
                "vendor/atk4/core/tests/"
            ],
            "atk4\\dsql\\tests\\": [
                "vendor/atk4/dsql/tests/"
            ],
            "atk4\\data\\tests\\": [
                "vendor/atk4/data/tests/"
            ],
            "atk4\\schema\\tests\\": [
                "vendor/atk4/schema/tests/"
            ],
            "atk4\\ui\\tests\\": [
                "vendor/atk4/ui/tests/"
            ]
        }
    }
}
