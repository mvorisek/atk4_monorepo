{
    "name": "atk4/monorepo",
    "description": "Monorepo of major Atk4 repos",
    "type": "library",
    "homepage": "https://www.mvorisek.com/",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Voříšek",
            "email": "mvorisek@mvorisek.cz"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "atk4/core": "dev-develop#503ed2ff35865ca8b5499ecb61c95a6257b95621",
        "atk4/dsql": "dev-develop#1d6ccd10d2fa17aa4f21f83ec086d3c446b00f44",
        "atk4/data": "dev-develop#c7b6de34cb0280f0d3374ae6587438ae1d693f96",
        "atk4/schema": "dev-develop#008909c8e5802cda4b9a6d736c601fba98e26f67",
        "atk4/ui": "dev-develop#34cc4117730bc80f746e7fbcf765223ac46e5b9b"
    },
    "require-dev": {
        "atk4/data": "dev-develop",
        "friendsofphp/php-cs-fixer": "^2.16, ^2.16, ^2.16, ^2.16",
        "phpunit/phpunit": "*, *, <6, *, *",
        "symfony/contracts": ">=1.1",
        "symfony/yaml": ">=3.4",
        "fzaninotto/faker": "*, *, *, *, *",
        "atk4/schema": "dev-develop",
        "phpunit/dbunit": ">=1.2",
        "atk4/ui": "dev-develop",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.3 || ^5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "atk4\\core\\tests\\": [
                "vendor/atk4/core/tests/"
            ],
            "atk4\\dsql\\tests\\": [
                "vendor/atk4/dsql/tests/"
            ],
            "atk4\\data\\tests\\": [
                "vendor/atk4/data/tests/"
            ],
            "atk4\\schema\\tests\\": [
                "vendor/atk4/schema/tests/"
            ],
            "atk4\\ui\\tests\\": [
                "vendor/atk4/ui/tests/"
            ]
        }
    }
}
